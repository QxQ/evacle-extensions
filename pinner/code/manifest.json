{
  "name": "pinner",
  "version": "0.3",
  "manifest_version": 2,
  "description": "adds a pin tab button so you don't have to right click to pin or unpin.",
  "icons": {"48": "icon48.png",
            "128": "icon128.png" },
  "background": {
  			"page":"background.html"},
  "permissions": ["tabs"],
  "browser_action": {
    "default_icon": "pin.png",
    "default_title": "pin tab"
  }
}
