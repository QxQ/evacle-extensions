{
  "name": "close pinned tab",
  "description": "puts a close tab button in the address bar when you are currently viewing a pinned tab.",
  "version": "0.1",
  "manifest_version":2,
  "icons": {"48": "icon48.png",
            "128": "icon128.png" },
  "background": {
  	"page": "background.html"
  },
  "permissions": ["tabs"],
  "page_action": {
    "default_icon": "close.png",
    "default_title": "close tab"
  }
}
